{
  "InventoryName": "HostDB",
  "Validation": {
    "Hostname": [
      {
        "Validator": "Fqdn",
        "Opt": ".co.uk"
      },
      {
        "Validator": "MaxChars",
        "Opt": 15
      }
    ],
    "OperatingSystem": [
      {
        "Validator": "OneOf",
        "Opt": [
          "Windows",
          "Linux"
        ]
      }
    ]
  }
}
